[[ -f /usr/share/mapcrafter/mc.jar ]] && rm /usr/share/mapcrafter/mc.jar
curl -o /usr/share/mapcrafter/mc.jar https://s3.amazonaws.com/Minecraft.Download/versions/1.12/1.12.jar
/usr/bin/mapcrafter_textures.py -f /usr/share/mapcrafter/mc.jar "/usr/share/mapcrafter/textures"
rm /usr/share/mapcrafter/mc.jar
